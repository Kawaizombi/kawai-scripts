{
  "name": "@kawai-scripts/kawai-userscripts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "bootstrap": "lerna bootstrap && lerna run build",
    "dev:base": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack-dev-server --config=build/webpack.dev.config.ts --mode=development",
    "build:base": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config=build/webpack.prod.config.ts --mode=production",
    "analyze:base": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config=build/webpack.analyzer.config.ts --mode=production",
    "dev:hover-zoom": "npm run dev:base -- --project=\"hover-zoom\"",
    "build:hover-zoom": "npm run build:base -- --project=\"hover-zoom\"",
    "dev:animespirit-ost-downloader": "npm run dev:base -- --project=\"animespirit-ost-downloader\"",
    "build:animespirit-ost-downloader": "npm run build:base -- --project=\"animespirit-ost-downloader\"",
    "analyze:animespirit-ost-downloader": "npm run analyze:base -- --project=\"animespirit-ost-downloader\"",
    "dev:youtube-blocker": "ng serve --no-live-reload --project youtube-blocker --single-bundle --extra-webpack-config projects/youtube-blocker/webpack.dev.config.js",
    "build:youtube-blocker": "ng build --prod --project youtube-blocker --single-bundle --extra-webpack-config projects/youtube-blocker/webpack.prod.config.js",
    "analyze:youtube-blocker": "ng build --prod --project youtube-blocker --single-bundle --extra-webpack-config projects/youtube-blocker/webpack.analyze.config.js",
    "dev:youtube-tweaks": "ng serve --no-live-reload --project youtube-tweaks --single-bundle --extra-webpack-config projects/youtube-tweaks/webpack.dev.config.js",
    "build:youtube-tweaks": "ng build --prod --project youtube-tweaks --single-bundle --extra-webpack-config projects/youtube-tweaks/webpack.prod.config.js",
    "analyze:youtube-tweaks": "ng build --prod --project youtube-tweaks --single-bundle --extra-webpack-config projects/youtube-tweaks/webpack.analyze.config.js",
    "dev:get-qrcode": "npm run dev:base -- --project=\"get-qrcode\"",
    "build:get-qrcode": "npm run build:base -- --project=\"get-qrcode\"",
    "dev:soundcloud-downloader": "ng serve --no-live-reload --project soundcloud-downloader --single-bundle --extra-webpack-config projects/soundcloud-downloader/webpack.dev.config.js",
    "build:soundcloud-downloader": "ng build --prod --project soundcloud-downloader --single-bundle --extra-webpack-config projects/soundcloud-downloader/webpack.prod.config.js",
    "analyze:soundcloud-downloader": "ng build --prod --project=soundcloud-downloader --single-bundle --extra-webpack-config projects/soundcloud-downloader/webpack.analyze.config.js"
  },
  "keywords": [],
  "workspaces": [
    "core/*",
    "projects/*"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.6",
    "@angular/cli": "^8.3.23",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "~8.2.14",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.14",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-server": "^3.9.0",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "eslint": "^6.7.2",
    "eslint-plugin-jest": "^23.1.1",
    "html-loader": "^0.5.5",
    "husky": "^3.1.0",
    "lerna": "^3.20.2",
    "lint-staged": "^9.5.0",
    "ngx-build-plus": "^9.0.2",
    "style-loader": "^1.0.2",
    "svg-inline-loader": "^0.8.0",
    "to-string-loader": "^1.1.6",
    "ts-node": "^8.5.4",
    "typescript": "3.5.3",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-userscript": "^2.4.0"
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "rxjs": "^6.5.4",
    "tslib": "^1.9.0",
    "zone.js": "^0.10.2"
  }
}
