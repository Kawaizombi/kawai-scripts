{
  "name": "@kawai-scripts/kawai-userscripts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "bootstrap": "lerna run build && lerna bootstrap",
    "dev:base": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack-dev-server --config=build/webpack.dev.config.ts --mode=development",
    "build:base": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config=build/webpack.prod.config.ts --mode=production",
    "analyze:base": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config=build/webpack.analyzer.config.ts --mode=production",
    "dev:hover-zoom": "npm run dev:base -- --project=\"hover-zoom\"",
    "build:hover-zoom": "npm run build:base -- --project=\"hover-zoom\"",
    "dev:animespirit-ost-downloader": "npm run dev:base -- --project=\"animespirit-ost-downloader\"",
    "build:animespirit-ost-downloader": "npm run build:base -- --project=\"animespirit-ost-downloader\"",
    "analyze:animespirit-ost-downloader": "npm run analyze:base -- --project=\"animespirit-ost-downloader\"",
    "dev:youtube-blocker": "npm run dev:base -- --project=\"youtube-blocker\"",
    "build:youtube-blocker": "npm run build:base -- --project=\"youtube-blocker\""
  },
  "keywords": [],
  "workspaces": [
    "core/*",
    "projects/*"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.14",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-server": "^3.9.0",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "eslint": "^6.7.2",
    "eslint-plugin-jest": "^23.1.1",
    "html-loader": "^0.5.5",
    "husky": "^3.1.0",
    "lerna": "^3.19.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "svg-inline-loader": "^0.8.0",
    "style-loader": "^1.0.2",
    "to-string-loader": "^1.1.6",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
