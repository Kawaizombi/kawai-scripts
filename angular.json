{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "youtube-blocker": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/youtube-blocker",
      "sourceRoot": "projects/youtube-blocker/src",
      "prefix": "yt-blocker",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist",
            "index": "",
            "main": "projects/youtube-blocker/src/index.ts",
            "tsConfig": "projects/youtube-blocker/tsconfig.json",
            "aot": false,
            "extractLicenses": false,
            "styles": [
              "projects/youtube-blocker/src/styles/main.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "deleteOutputPath": false,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "sourceMap": false,
            "aot": true,
            "browserTarget": "youtube-blocker:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "youtube-blocker:build:production"
            }
          }
        }
      }
    },
    "youtube-tweaks": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/youtube-tweaks",
      "sourceRoot": "projects/youtube-tweaks/src",
      "prefix": "yt-tweaks",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "extractLicenses": false,
            "outputPath": "dist",
            "index": "",
            "main": "projects/youtube-tweaks/src/main.ts",
            "tsConfig": "projects/youtube-tweaks/tsconfig.app.json",
            "styles": [
              "projects/youtube-tweaks/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/youtube-tweaks/src/environments/environment.ts",
                  "with": "projects/youtube-tweaks/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "deleteOutputPath": false,
              "outputHashing": "none",
              "sourceMap": false,
              "es5BrowserSupport": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "aot": true,
            "es5BrowserSupport": false,
            "browserTarget": "youtube-tweaks:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "youtube-tweaks:build:production"
            }
          }
        }
      }
    }
  }
}
